{
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      },
      {
        "src": "/api/novela",
        "methods": ["POST", "GET"],
        "dest": "/api/novela.js"
      }
    ]
  }