{
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      },
      {
        "src": "./Controller/TaskController.js",
        "methods": ["POST", "GET","PUT"],
        "dest": "/api/task"
      }
    ]
  }